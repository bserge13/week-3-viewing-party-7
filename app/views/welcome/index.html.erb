<% if current_user %>
  <%= link_to "Log Out" %><p>

<section class='existing-users'>
  <p>Existing Users: </p>
  <% @users.each do |user| %>
      <% if current_user.admin? %>
      <li><%= link_to user.email, user_path(user.id) %></li>
    <% else %>  
      <li><%= user.email %></li>
    <% end %>
  <% end %>
</section>
<% else %>
  <%= button_to "Create New User", register_path, method: :get %>
  <%= link_to "Log In", '/login' %><br>
<% end %>